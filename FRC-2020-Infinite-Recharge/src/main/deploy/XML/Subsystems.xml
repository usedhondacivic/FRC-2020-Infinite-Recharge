<?xml version="1.0"?>
<robot>
    <CHASSIS>
        <group id="LEFT_SIDE">
            <motor controller="TALON" port="1"></motor>
            <motor controller="TALON" port="2"></motor>
        </group>
        <group id="RIGHT_SIDE">
            <motor controller="TALON" port="3"></motor>
            <motor controller="TALON" port="4"></motor>
        </group>
    </CHASSIS>
    <SHOOTER>
        <motor id="SHOOTER_WHEEL" controller="SPARK_MAX" port="5"></motor>
        <motor id="TURRET_AIM" controller="TALON" port="6"></motor>
        <solenoid id="HOOD_ADJUST" type="SINGLE" port="0"></solenoid>
    </SHOOTER>
    <CLIMBER>
        <motor id="WINCH" controller="TALON" port="7"></motor>
    </CLIMBER>
    <INTAKE>
        <solenoid id="INTAKE_RAISE" type="SINGLE" port="1"></solenoid>
        <motor id="INTAKE_WHEELS" controller="TALON" port="8"></motor>
        <motor id="INDEXER_RIGHT" controller="TALON" port="9"></motor>
        <motor id="INDEXER_LEFT" controller="TALON" port="10"></motor>
        <motor id="INDEXER_CENTER" controller="TALON" port="11"></motor>
    </INTAKE>
    <PANNEL>
        <motor id="PANNEL_SPINNER" controller="TALON" port="12"></motor>
        <solenoid id="PANNEL_RAISE" type="SINGLE" port="2"></solenoid>
    </PANNEL>
</robot>